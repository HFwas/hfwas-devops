<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hfwas.devops.mapper.DevopsSyncLogMapper">

    <resultMap id="actorResultMap" type="com.hfwas.devops.entity.DevopsSyncLog">
        <id column="id" property="id"/>
        <result column="type" property="type"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="success_num" property="successNum"/>
        <result column="success_data" property="successData"/>
        <result column="error_num" property="errorNum"/>
        <result column="error_data" property="errorData"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type, status,create_time, update_time,success_num,success_data,error_num,error_data
    </sql>



</mapper>